{
    "name": "onefit/events",
    "description": "Events handling with kafka",
    "keywords": ["laravel", "lumen", "events", "kafka"],
    "type": "library",
    "authors": [
        {
            "name": "Stefan Djokic",
            "email": "stefan@one.fit",
            "homepage": "https://one.fit",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-rdkafka": "^4.0",
        "ext-pcntl": "^7.2",
        "ext-json": "^1.6",
        "flix-tech/avro-serde-php": "^1.3",
        "illuminate/support": "^5.8 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "kwn/php-rdkafka-stubs": "^2.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "OneFit\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneFit\\Events\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OneFit\\Events\\EventsServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "stable"
}
